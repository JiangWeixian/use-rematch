{
  "name": "use-rematch",
  "version": "0.0.0",
  "author": {
    "name": "jiangweixian"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JiangWeixian/use-rematch"
  },
  "license": "MIT",
  "keywords": [
    "rematch",
    "rematch2",
    "redux",
    "hooks",
    "useReducer",
    "useRematchReducer"
  ],
  "peerDependencies": {
    "react": ">=16.9.0"
  },
  "devDependencies": {
    "lerna": "^3.20.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "scripts": {
