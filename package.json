{
  "name": "use-rematch",
  "version": "0.0.0",
  "author": {
    "name": "jiangweixian"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JiangWeixian/use-rematch"
  },
  "license": "MIT",
  "keywords": [
    "rematch",
    "rematch2",
    "redux",
    "hooks",
    "useReducer",
    "useRematchReducer",
    "useRematch"
  ],
  "peerDependencies": {
    "react": ">=16.9.0"
  },
  "devDependencies": {
    "@aiou/eslint-config": "^0.0.12",
    "eslint": "^7.29.0",
    "lerna": "3.20.2",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.1",
    "typescript": "3.8.3"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap --nohoist=@use-rematch/core",
    "test": "lerna run test",
    "build": "lerna run build",
    "publish": "npm run build && lerna publish",
    "prettier": "prettier --config .prettierrc --write '**/*'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {}
}
